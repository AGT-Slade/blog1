
<%- include("partials/header"); -%>
    <h1>Home</h1>
    <p> <%= startingContent %> </p>



  <%  posts.forEach(function(post){ %>

    <h1><%=post.title%></h1>
    <p>
    <%=post.content.substring(0, 100) + " ..."%>
    <a href="/posts/<%=post.title%>">Read More</a>
    </p>

    <form class="" action="/deletePost" method="post">
      <button type="submit" name="deletePost" value= "<%=post.title%>">Delete Post</button>
    </form>

    <% }) %>


<%- include("partials/footer"); -%>
